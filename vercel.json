{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "npm run build"
            }
        },
        {
            "src": "server-build/index.js",
            "use": "@vercel/node"
        }
    ],
}
